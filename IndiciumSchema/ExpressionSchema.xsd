<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="https://github.com/mamift/Indicium"
    targetNamespace="https://github.com/mamift/Indicium" elementFormDefault="qualified">
    <xs:include id="TokenDefinition" schemaLocation="TokenSchema.xsd"/>
    <xs:element name="Expression">
        <xs:complexType>
            <xs:choice>
                <xs:sequence maxOccurs="unbounded" minOccurs="0">
                    <xs:element ref="Token" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:choice>
        </xs:complexType>
    </xs:element>
</xs:schema>
