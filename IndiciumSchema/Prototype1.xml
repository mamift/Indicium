<?xml version="1.0" encoding="UTF-8"?>
<TokenContext xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://github.com/mamift/Indicium TokenSchema.xsd"
 xmlns="https://github.com/mamift/Indicium">
  <Token Id="QuotationMark">\"</Token>
  <Token Id="SingleQuotationMark">\'</Token>
  <Token Id="Whitespace">[\t\ ]+</Token>
  <Token Id="Digit">\d</Token>
  <Token Id="Dot">\.</Token>
  <Token Id="Comma">\,</Token>
  <Token Id="Plus">\+</Token>
  <Token Id="Equals">\=</Token>
  <Token Id="Colon">\:</Token>
  <Token Id="Semicolon">\;</Token>
  <Token Id="Identifier">[a-zA-Z_][a-zA-Z0-9_]*</Token>
</TokenContext>
